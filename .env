# ============================================
# SOLANA RPC ENDPOINTS
# ============================================
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com

# Backup RPC endpoints (optional but recommended)
SOLANA_RPC_URL_BACKUP_1=
SOLANA_RPC_URL_BACKUP_2=
SOLANA_RPC_URL_BACKUP_3=

# ============================================
# WALLET CONFIGURATION
# ============================================
# Your bot wallet private key (Base58 encoded)
WALLET_PRIVATE_KEY=your_base58_private_key_here

# Additional wallets (optional - up to 10 total)
WALLET_PRIVATE_KEY_2=optional
WALLET_PRIVATE_KEY_3=optional
WALLET_PRIVATE_KEY_4=optional
WALLET_PRIVATE_KEY_5=optional
WALLET_PRIVATE_KEY_6=optional
WALLET_PRIVATE_KEY_7=optional
WALLET_PRIVATE_KEY_8=optional
WALLET_PRIVATE_KEY_9=optional
WALLET_PRIVATE_KEY_10=optional

# Phantom wallet address (where earnings go)
PHANTOM_WALLET_ADDRESS=your_phantom_wallet_address_here

# ============================================
# TELEGRAM CONFIGURATION
# ============================================
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_CHAT_ID=your_telegram_chat_id_here

# ============================================
# AUTO-WITHDRAW SETTINGS
# ============================================
AUTO_WITHDRAW=true
WITHDRAW_THRESHOLD=0.1

# ============================================
# PERFORMANCE SETTINGS
# ============================================
SCAN_INTERVAL=2000
MAX_CONCURRENT_CLAIMS=50

# ============================================
# AI CONFIGURATION
# ============================================
AI_LEARNING_ENABLED=true
AUTO_ERROR_RESOLUTION=true
SKIP_PROBLEMATIC_TOKENS=true
MAX_AI_RETRIES=5
AI_CONFIDENCE_THRESHOLD=20

# ============================================
# LOGGING
# ============================================
LOG_LEVEL=INFO
ENABLE_DEBUG_LOGS=false
